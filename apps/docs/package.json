{
  "name": "scholar-mcp-docs",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "sync:api": "node ../../scripts/generate-docs-api.mjs",
    "sync:releases": "node ../../scripts/generate-docs-releases.mjs",
